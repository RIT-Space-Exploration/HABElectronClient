{
  "name": "hab-electron-client",
  "version": "1.0.0",
  "description": "Thin client for capturing serial output of HAB project",
  "main": "./src/main.js",
  "scripts": {
    "test": "eslint ./src",
    "start": "electron .",
    "postinstall": "cd node_modules/serialport && node-pre-gyp rebuild --target=1.3.4 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "rebuild:64": "cd node_modules/serialport && node-pre-gyp rebuild --target=1.3.4 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "rebuild:32": "cd node_modules/serialport && node-pre-gyp rebuild --target=1.3.4 --arch=ia32 --dist-url=https://atom.io/download/atom-shell"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RIT-Space-Exploration/HABElectronClient.git"
  },
  "author": "RIT Space Exploration",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RIT-Space-Exploration/HABElectronClient/issues"
  },
  "homepage": "https://github.com/RIT-Space-Exploration/HABElectronClient#readme",
  "devDependencies": {
    "electron-prebuilt": "^1.3.4",
    "electron-rebuild": "^1.2.0",
    "node-gyp": "^3.4.0",
    "node-pre-gyp": "^0.6.29",
    "serialport": "^4.0.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2"
  },
  "dependencies": {
    "serialport": "^4.0.6",
    "socket.io": "^1.5.1"
  }
}
